{
  "name": "elegancecms/git-commit-checker",
  "description": "Check coding standard & code syntax with Git pre-commit hook.",
  "type": "package",
  "license": "MIT",
  "keywords": [
    "elegancecms",
    "elegancecms cms",
    "elegancecms platform",
    "elegancecms git commit checker"
  ],
  "homepage": "https://elegancecms.com",
  "authors": [
    {
      "name": "Sang Nguyen",
      "email": "sangnguyenplus@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "EleganceCMS\\GitCommitChecker\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "EleganceCMS\\GitCommitChecker\\Tests\\": "tests"
    }
  },
  "require": {
    "php": "^8.0|^8.1|^8.2|^8.3",
    "laravel/framework": ">=9.32",
    "laravel/pint": "^1.5"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0|^10.0|^11.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "EleganceCMS\\GitCommitChecker\\Providers\\GitCommitCheckerServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "stable"
}
